        android:name="net.ankio.auto.ui.fragment.BookEditFragment"
        android:label="@string/title_book_edit"
        tools:layout="@layout/fragment_book_edit">

        <argument
            android:name="bookId"
            app:argType="long"
            android:defaultValue="0L" />
    </fragment>

    <fragment
        android:id="@+id/categoryFragment"
        android:name="net.ankio.auto.ui.fragment.CategoryFragment"
