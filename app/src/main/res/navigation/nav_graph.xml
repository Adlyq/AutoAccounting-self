        tools:layout="@layout/fragment_plugin_home">


        <action
            android:id="@+id/action_homeFragment_to_logFragment"
            app:destination="@id/logFragment" />
        <action
            android:id="@+id/action_homeFragment_to_bookFragment"
            app:destination="@id/bookFragment" />
        <action
            android:id="@+id/action_homeFragment_to_categoryFragment"
            app:destination="@id/categoryFragment" />
        <action
            android:id="@+id/action_homeFragment_to_categoryMapFragment"
            app:destination="@id/categoryMapFragment" />
        <action
            android:id="@+id/action_homeFragment_to_assetFragment"
            app:destination="@id/assetFragment" />
        <action
            android:id="@+id/action_homeFragment_to_assetMapFragment"
            app:destination="@id/assetMapFragment" />
        <action
            android:id="@+id/action_homeFragment_to_tagFragment"
            app:destination="@id/tagFragment" />
        <action
            android:id="@+id/action_homeFragment_to_orderFragment"
            app:destination="@id/orderFragment" />
        <action
            android:id="@+id/action_homeFragment_to_dataFilterFragment"
            app:destination="@id/dataFilterFragment" />
        <action
