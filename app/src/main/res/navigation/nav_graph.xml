        android:name="net.ankio.auto.ui.fragment.CategoryEditFragment"
        android:label="@string/title_category_edit"
        tools:layout="@layout/fragment_category_edit">

        <argument
            android:name="categoryId"
            app:argType="long"
            android:defaultValue="0L" />
        <argument
            android:name="bookName"
            app:argType="string"
            android:defaultValue="" />
        <argument
            android:name="billType"
            app:argType="string"
            android:defaultValue="Expend" />
        <argument
            android:name="parentId"
            app:argType="string"
            android:defaultValue="-1" />
    </fragment>

